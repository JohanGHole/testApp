{
  "name": "app-template",
  "version": "1.0.0",
  "description": "",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "start": "concurrently --names \"CSS,6PS\" \"yarn start:css\" \"d2-app-scripts start\" --hide \"CSS\"",
    "start:css": "tailwindcss -o src/tailwind.css --watch",
    "build": "yarn build:css && d2-app-scripts build",
    "build:css": "NODE_ENV=production npx tailwindcss -o src/tailwind.css --minify",
    "test": "d2-app-scripts test",
    "deploy": "d2-app-scripts deploy",
    "lint": "eslint -c .eslintrc.json . --quiet",
    "cy:open": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && cypress open --env dhis2BaseUrl=http://localhost:8080\"",
    "cy:run": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && cypress run --env dhis2BaseUrl=http://localhost:8080\"",
    "cy:run-ct": "build:css && cypress run --component",
    "i18n:add": "d2-app-scripts i18n extract && git add ./i18n/",
    "tsc:check": "tsc",
    "deploy:test": "yarn build && yarn deploy"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.18.6",
    "@badeball/cypress-cucumber-preprocessor": "^13.0.0",
    "@cypress/react": "^6.2.0",
    "@cypress/webpack-preprocessor": "5.16.2",
    "@dhis2/cli-app-scripts": "^10.0.0",
    "@dhis2/d2-i18n": "^1.1.0",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.7",
    "cypress": "^10.8.0",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.8.3",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "@dhis2/app-runtime": "^3.5.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@headlessui/react": "^1.7.2",
    "@mui/material": "^5.10.7",
    "@popperjs/core": "^2.11.6",
    "@reduxjs/toolkit": "^1.8.6",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.8.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.3.0",
    "formik": "^2.2.9",
    "react-popper": "^2.3.0",
    "react-query": "^3.39.1",
    "react-query-devtools": "^2.6.3",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.3.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "recharts": "^2.1.10",
    "yup": "^0.32.11"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
